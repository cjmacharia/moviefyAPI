{
	"name": "moviefy",
	"version": "1.0.0",
	"description": "A node application that takes a movie from OMDB and interacts with spotify to get all the soundtracks realted to the movie",
	"main": "app.js",
	"scripts": {
		"test": "mocha --require babel-core/register ./tests/**/*.js",
		"start": "nodemon --exec babel-node app.js",
		"coverage": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- ./tests/** --compilers js:babel-register",
		"coveralls": "npm run coverage && cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cjmash/moviefy.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/cjmash/moviefy/issues"
	},
	"homepage": "https://github.com/cjmash/moviefy#readme",
	"dependencies": {
		"@babel/register": "^7.0.0",
		"babel-cli": "^6.26.0",
		"babel-plugin-istanbul": "^5.0.1",
		"babel-preset-stage-3": "^6.24.1",
		"bcrypt": "3.0.0",
		"body-parser": "^1.18.3",
		"chai": "^4.2.0",
		"chai-http": "^4.2.0",
		"coveralls": "^3.0.2",
		"express": "^4.16.3",
		"fetch": "^1.1.0",
		"istanbul": "^0.4.5",
		"jsonwebtoken": "^8.3.0",
		"mocha": "^5.2.0",
		"mongoose": "^5.3.1",
		"mongoose-unique-validator": "^2.0.2",
		"morgan": "^1.9.1",
		"node-fetch": "^2.2.0",
		"validator": "^10.8.0"
	},
	"devDependencies": {
		"@babel/preset-es2015": "^7.0.0-beta.53",
		"@babel/preset-es2017": "^7.0.0-beta.53",
		"babel-core": "^6.26.3",
		"babel-istanbul": "^0.12.2",
		"babel-loader": "^8.0.4",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"nyc": "^13.0.1"
	}
